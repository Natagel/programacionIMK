#include <stdio.h>

// Suma de los numeros naturales del 1 al n

int main (void)
{
    int n=0;
    int i=1;
    int suma=0;

    printf ("Ingresa el valor de n: \n");
    scanf ("%d", &n);
    printf ("La suma de los numeros naturales es: \n");

    while (i<=n)
    {
        suma=suma+i;
         printf ("%d \n", suma);
         i++;
    }
    return (0);
}